{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "CupaApplication",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "lt.creditco.cupa.CupaApplication",
      "projectName": "cupa",
      "args": "",
      "envFile": "${workspaceFolder}/.env",
      "vmArgs": "-Dspring.profiles.active=dev -Dspring.jmx.enabled=false ",
      "preLaunchTask": "cleanup-vapp-base-artifacts"
    },
    {
      "type": "java",
      "name": "CupaApplication hotswap",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "lt.creditco.cupa.CupaApplication",
      "projectName": "cupa",
      "args": "",
      "envFile": "${workspaceFolder}/.env",
      "vmArgs": "-XX:+AllowEnhancedClassRedefinition -Dspring.profiles.active=dev -Dspring.jmx.enabled=false ",
      "preLaunchTask": "cleanup-vapp-base-artifacts"
    },
    {
      "type": "java",
      "name": "CupaApplication dev-pg",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "lt.creditco.cupa.CupaApplication",
      "projectName": "cupa",
      "args": "",
      "envFile": "${workspaceFolder}/.env",
      "vmArgs": "-Dspring.profiles.active=dev-pg -Dspring.jmx.enabled=false ",
      "preLaunchTask": "cleanup-vapp-base-artifacts"
    },
    {
      "type": "java",
      "name": "CupaApplication dev-pg hotswap",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "lt.creditco.cupa.CupaApplication",
      "projectName": "cupa",
      "args": "",
      "envFile": "${workspaceFolder}/.env",
